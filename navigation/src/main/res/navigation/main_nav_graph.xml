<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/main_nav_graph"
    app:startDestination="@id/blankMainFragment">

    <include app:graph="@navigation/library" />
    <include app:graph="@navigation/unknown" />
    <include app:graph="@navigation/profile" />
    <fragment
        android:id="@+id/blankMainFragment"
        android:name="ru.dashkevich.navigation.BlankMainFragment"
        android:label="fragment_blank_main"
        tools:layout="@layout/fragment_blank_main" >
        <action
            android:id="@+id/action_blankMainFragment_to_library"
            app:destination="@id/library"
            app:popUpTo="@id/blankMainFragment"
            app:popUpToInclusive="true" />
    </fragment>

</navigation>
