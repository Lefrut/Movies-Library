<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/root_nav_graph"
    tools:ignore="InvalidNavigation"
    app:startDestination="@id/blankFragment">

    <include app:graph="@navigation/start_nav_graph" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="ru.dashkevich.navigation.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_start_nav_graph"
            app:destination="@id/start_nav_graph"
            app:popUpTo="@id/blankFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <include app:graph="@navigation/main" />
    <action
        android:id="@+id/action_global_main"
        app:destination="@id/main"
        app:popUpTo="@id/start_nav_graph"
        app:popUpToInclusive="true"
        app:launchSingleTop="true" />
    <action
        android:id="@+id/action_global_blankFragment"
        app:destination="@id/blankFragment"
        app:popUpTo="@id/start_nav_graph"
        app:popUpToInclusive="true"
        app:launchSingleTop="true" />
</navigation>