<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/root_nav_graph"
    tools:ignore="InvalidNavigation"
    app:startDestination="@navigation/library">

    <include app:graph="@navigation/start_nav_graph" />
    <include app:graph="@navigation/main_nav_graph" />
    <action
        android:id="@+id/action_global_main_nav_graph"
        app:destination="@id/main_nav_graph"
        app:popUpTo="@id/start_nav_graph"
        app:popUpToInclusive="true"
        app:launchSingleTop="true" />
    <include app:graph="@navigation/library" />
</navigation>